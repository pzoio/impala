<root>
	<context-locations>
		<context-location>parent-context.xml</context-location>
		<context-location>merchant-context.xml</context-location>
	</context-locations>
	<modules>
		<module>
			<name>wineorder-hibernate</name>
		</module>
		<module>
			<name>wineorder-dao</name>
		</module>
		<module>
			<name>wineorder-merchant</name>
			<type>application_with_beansets</type>
			<overrides>alternative: myImports</overrides>
		</module>
		<module>
			<name>wineorder-web</name>
			<context-locations>
				<context-location>wineorder-web.xml</context-location>
			</context-locations>
			<type>WEB_ROOT</type>
			<modules>
				<module>
					<name>wineorder-servlet1</name>
					<context-locations>
						<context-location>wineorder-servlet1-context.xml</context-location>
					</context-locations>
					<type>SERVLET</type>
				</module>
				<module>
					<name>wineorder-servlet2</name>
					<context-locations>
						<context-location>wineorder-servlet2-context.xml</context-location>
					</context-locations>
					<type>SERVLET</type>
				</module>
			</modules>
		</module>
	</modules>
</root>