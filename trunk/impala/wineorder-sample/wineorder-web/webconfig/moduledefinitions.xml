<root>
	<names>
	example-hibernate
	example-dao
	example-service
	example-web
	example-servlet1
	example-servlet2
	example-servlet3
	</names>
	<modules>
		<module>
			<name>example-service</name>
			<type>application_with_beansets</type>
			<overrides>alternative: myImports</overrides>
		</module>	
		<module>
			<name>example-web</name>
			<context-locations>
				<context-location>example-web.xml</context-location>
			</context-locations>
			<type>WEB_ROOT</type>
		</module>
		<module>
			<name>example-servlet1</name>
			<context-locations>
				<context-location>example-servlet1-context.xml</context-location>
			</context-locations>
			<type>SERVLET</type>
		</module>
		<module>
			<name>example-servlet2</name>
			<context-locations>
				<context-location>example-servlet2-context.xml</context-location>
			</context-locations>
			<type>SERVLET</type>
		</module>
		<module>
			<name>example-servlet3</name>
			<context-locations>
				<context-location>example-servlet3-context.xml</context-location>
			</context-locations>
			<type>SERVLET</type>
		</module>
	</modules>
</root>