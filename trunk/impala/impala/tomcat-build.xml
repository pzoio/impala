<?xml version="1.0"?>
<project name="Impala Tomcat Build">
    
    <target 
        name="tomcat:copy">
        <property name = "tomcat.war.file" value = "${tomcat.home}/webapps/${project.name}.war"/>
        
        <echo message = "Tomcat war file ${tomcat.war.file}"/>
        <copy file="${war.output.file}" tofile = "${tomcat.war.file}"/>
    </target>   
    
</project>
