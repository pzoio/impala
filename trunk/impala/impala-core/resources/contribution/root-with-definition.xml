<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
 
 	<bean id="serviceRegistry" class="org.impalaframework.service.registry.internal.ServiceRegistryImpl"/>
	
	<bean id="child"
		class="org.impalaframework.spring.service.proxy.ContributionProxyFactoryBean">
		<property name="proxyInterfaces" value="org.impalaframework.spring.module.impl.Child"/>
		<property name="serviceRegistry" ref="serviceRegistry"/>
	</bean>
	
	<bean id="another"
		class="org.impalaframework.spring.service.proxy.ContributionProxyFactoryBean">
		<property name="proxyInterfaces" value="org.impalaframework.spring.module.impl.Child"/>
		<property name="serviceRegistry" ref="serviceRegistry"/>
	</bean>
	
</beans>