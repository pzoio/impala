<web-app>
	<listener>
		<listener-class>org.impalaframework.plugin.web.ImpalaContextLoaderListener</listener-class>
	</listener>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>parent-context.xml</param-value>
	</context-param>
	<context-param>
		<param-name>pluginNames</param-name>
		<param-value>wineorder-dao, wineorder-hibernate, wineorder-merchant ()</param-value>
	</context-param>
	<context-param>
		<param-name>webappConfigLocation</param-name>
		<param-value>wineorder-web.xml</param-value>
	</context-param>	
    <servlet>
        <servlet-name>wineorder-web</servlet-name>
        <servlet-class>org.impalaframework.plugin.web.ImpalaServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>wineorder-web</servlet-name>
        <url-pattern>/web/*.do</url-pattern>
    </servlet-mapping>
